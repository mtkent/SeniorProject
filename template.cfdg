startshape blah
CF::Background = [hue 102 sat 1 b -0.3070448836582896 ]
CF::MinimumSize = .1

shape blah {
 blah2 [alpha -10]
 blah2 [flip 90 alpha -1 x 5 b 1]
 blah2 [alpha -1 y -5 b 1]
